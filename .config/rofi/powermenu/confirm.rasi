@import "~/.cache/wal/colors-rofi-dark"

* {
    background-color:       @background;
    text-color:             @foreground;
	font:				 	"Iosevka Nerd Font 15";
}

window {
    width:      			240px;
    padding:    			15px;
    border:					0px;
    border-radius:          0px;
    border-color:           @background;
    location:               center;
    y-offset:               -20px;
}

entry {
    expand: 				true;
    text-color:				@primary;
}

